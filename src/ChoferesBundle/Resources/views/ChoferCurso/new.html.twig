{% extends 'ChoferesBundle:Layout:layout.html.twig' %}

{% block javascript %}
    <script src="{{ asset('bundles/choferes/js/autocompletar.js') }}"></script>
{% endblock %}

{% block title %}
{{ parent() }} - {{ 'views.new.creation'|trans({'%entity%': 'ChoferCurso'}, 'JordiLlonchCrudGeneratorBundle') }}
{% endblock %}

{% block menu %}
{{ include('ChoferesBundle:NavBars:navbar.html.twig') }}
{% endblock %}

{% block page %}

<h1>{{ 'views.new.creation'|trans({'%entity%': 'ChoferCurso'}, 'JordiLlonchCrudGeneratorBundle') }}</h1>
<input value="" id="autocompletarInput">
<div id="autocompletarDiv"></div>
<form class="well" action="{{ path('chofercurso_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit" class="btn btn-success">{{ 'views.new.create'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}</button>
    </p>
</form>

<div>
    <div class="float-left">
        <a class="btn" href="{{ path('chofercurso') }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
</div>
{% endblock %}
