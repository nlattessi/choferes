{% extends 'ChoferesBundle:Layout:layout.html.twig' %}

{% block title %}
{{ parent() }} - {{ 'views.show.show'|trans({'%entity%': 'UsuarioPrestador'}, 'JordiLlonchCrudGeneratorBundle') }}
{% endblock %}

{% block menu %}
{{ include('ChoferesBundle:NavBars:navbar.html.twig') }}
{% endblock %}

{% block page %}

    <!-- Page -->
    <div class="animsition">
        <div class="page-header">
            <h1 class="page-title">Ver prestador</h1>
            <ol class="breadcrumb">
                <li><a href='{{ path('usuarioprestador') }}'>Prestadores</a></li>
                <li class="active">Ver</li>
            </ol>
        </div>
        <div class="page-content">

            <!-- Panel Table Add Row -->
            <div class="panel">
                <div class="panel-body">

                    <table class="table table-bordered table-hover table-striped" id="exampleAddRow">
                        <tbody>
                        <tr class="gradeA">
                            <td><b>ID</b></td>
                            <td>{{ entity.id }}</td>
                        </tr>
                        <tr class="gradeA">
                            <td><b>Nombre</b></td>
                            <td>{{ entity.usuario }}</td>
                        </tr>
                        <tr class="gradeA">
                            <td><b>Prestador</b></td>
                            <td>{{ entity.prestador }}</td>
                        </tr>
                        </tbody>
                    </table>

                    <div class="dt-panelfooter clearfix">
                        <button type="button" onclick="location.href='{{ path('usuarioprestador_edit', { 'id': entity.id }) }}'" class="btn btn-success margin-top-10">Editar</button>
                        <button type="button" onclick="location.href='{{ path('usuarioprestador') }}'" class="btn btn-outline btn-default margin-top-10">Volver al listado</button>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <!-- End Page -->


{% endblock %}
